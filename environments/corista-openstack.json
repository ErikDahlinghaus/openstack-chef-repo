      "memcached_servers": [
        "192.168.200.10:11211"
      ],
      "image": {
        "image_upload": true,
        "api": {
          "default_store": "file"
        },
        "upload_images": [ "cirros" ]
      },
      "db": {
        "compute_cell0": {
          "username": "nova"
        }
      },
      "compute": {
        "platform": {
          "compute_api_packages": [ "nova-api" ],
          "compute_api_service": "nova-api"
        },
        "conf": {
          "DEFAULT": {
            "log_dir": ""
          },
          "console": {
            "allowed_origins": "controller, controller.corista.local, 192.168.200.10"
          },
          "api": {
            "auth_strategy": "keystone"
          },
          "libvirt": {
            "virt_type": "qemu"
          },
          "vnc": {
            "enabled": true,
            "vncserver_proxyclient_address": "$my_ip",
            "novncproxy_base_url": "http://192.168.200.10:6080/vnc_auto.html"
          }
        }
      },
      "dashboard": {
        "server_hostname": "controller",
        "static_path": "/var/lib/openstack-dashboard/static",
        "neutron": {
          "enable_router": false,
          "enable_quotas": false,
          "enable_distributed_router": false,
          "enable_fip_topology_check": false,
          "enable_ha_router": false,
          "enable_lb": false,
          "enable_firewall": false,
          "enable_vpn": false
        }
      },
      "mq": {
        "user": "admin"
      },
      "network":{
        "interface_driver": "neutron.agent.linux.interface.BridgeInterfaceDriver",
        "plugins": {
          "ml2": {
            "conf": {
              "ml2": {
                "type_drivers": "flat,vlan",
                "tenant_network_types": "",
                "extension_drivers": "port_security,dns"
              },
              "ml2_type_flat": {
                "flat_networks": "provider"
              },
              "ml2_type_vlan": {
                "network_vlan_ranges": "provider"
              },
              "securitygroup": {
                "enable_ipset": true
              },
              "linux_bridge": {
                "physical_interface_mappings": "provider:br-vlan"
              }
            }
          },
          "linuxbridge": {
            "path": "/etc/neutron/plugins/ml2",
            "filename": "linuxbridge_agent.ini",
            "conf": {
              "linux_bridge": {
                "physical_interface_mappings": "provider:br-vlan"
              },
              "vxlan": {
                "enable_vxlan": false
              },
              "securitygroup": {
                "enable_security_group": true,
                "firewall_driver": "neutron.agent.linux.iptables_firewall.IptablesFirewallDriver"
              }
            }
          }
        },
        "conf": {
          "DEFAULT": {
            "dns_domain": "corista.services"
          },
          "neutron": {
            "service_metadata_proxy": true
          }
        },
        "platform": {
          "neutron_linuxbridge_agent_packages": [ "neutron-linuxbridge-agent" ],
          "neutron_linuxbridge_agent_service": "neutron-linuxbridge-agent"
        }
      },
      "network_dhcp": {
        "conf": {
          "DEFAULT": {
            "interface_driver": "linuxbridge",
            "enable_isolated_metadata": true
          }
        }
      },
      "network_metadata": {
        "conf": {
          "DEFAULT": {
            "nova_metadata_ip": "192.168.200.10"
          }
        }
      }
