{
  "name": "corista-openstack-controller-common",
  "description": "Setup up Corista Openstack Controller common",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "openstack": {
      "region": "Somerville",
      "apt": {
        "update_apt_cache": true
      },
      "sysctl": {
        "net.ipv4.ip_forward": 1
      },
      "secret": {
        "key_path": "/opt/corista/openstack-chef-repo/.chef/encrypted_data_bag_secret"
      },
      "bind_service": {
        "db": { "host": "0.0.0.0"},
        "mq": { "host": "0.0.0.0"}
      },
      "endpoints": {
        "db": { "host": "192.168.201.10" },
        "mq": { "host": "192.168.201.10" }
      }
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[openstack-common]",
    "recipe[openstack-common::logging]",
    "recipe[openstack-common::sysctl]",
    "recipe[openstack-common::client]",
    "recipe[corista-openstack::setup_db]",
    "recipe[openstack-ops-database::openstack-db]",
    "recipe[openstack-ops-messaging::rabbitmq-server]"
  ],
  "env_run_lists": {
  }
}
